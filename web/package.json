{
  "name": "console-dashboards-plugin",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:openshift/console-dashboards-plugin.git",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && NODE_ENV=production npm run ts-node node_modules/.bin/webpack",
    "build-dev": "npm run clean && npm run ts-node node_modules/.bin/webpack",
    "start": "npm run ts-node node_modules/.bin/webpack serve",
    "start-console": "./start-console.sh",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "^0.0.20",
    "@openshift-console/dynamic-plugin-sdk-webpack": "^0.0.10",
    "@patternfly/react-core": "4.221.3",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.11",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.76.3",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.15.1"
  },
  "resolutions": {
    "minimatch": "^3.0.5"
  },
  "consolePlugin": {
    "name": "console-dashboards-plugin",
    "version": "0.0.1",
    "displayName": "Dashboards console plugin",
    "description": "Plugin to add proxy configurations from console to in-cluster services to populate dashboards",
    "exposedModules": {
      "ProxyTestPage": "./components/ProxyTestPage",
      "GetDatasource": "./getDatasource"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "dependencies": {
    "lru-cache": "^7.14.1"
  }
}
